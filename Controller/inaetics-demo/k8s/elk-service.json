{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "elk-service"
  },
  "spec": {
    "ports": [
      {
        "name": "elasticsearch",
        "protocol": "TCP",
        "port": 9200,
        "targetPort": 9200,
        "nodePort": 0
      },
      {
        "name": "logstash",
        "protocol": "TCP",
        "port": 5000,
        "targetPort": 5000,
        "nodePort": 0
      },
      {
        "name": "kibana",
        "protocol": "TCP",
        "port": 5601,
        "targetPort": 5601,
        "nodePort": 0
      }
    ],
    "selector": {
      "name": "elk-pod"
    },
    "type": "ClusterIP",
    "deprecatedPublicIPs": [
      "172.17.8.20"
    ],
    "sessionAffinity": "None"
  }
}